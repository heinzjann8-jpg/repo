<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza/"
     xml:base="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:pizza="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza">
        <rdfs:comment xml:lang="en">pizza ontology implemented and featuring SWRL, ordered enumerated classes, inferencing, inversed properties, cardinal, existential and universal restriction. by Heinrick Janiola</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaMoreSpicyThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaMoreSpicyThan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isIngredientOf"/>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isMilderThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isMilderThan">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isSpicierThan"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isSpicierThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isSpicierThan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasCaloricContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that has Mozzarella, Tomato, Pepperoni, Jalapeno toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that has Moozzarella, Tomato, and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#LowCaloricPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#LowCaloricPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A pizza that only has Mozzarella and Tomato Topping</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MozzarellaTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheeseTopping"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#ParmesanTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MushroomTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MushroomTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#NamedPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#NamedPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#OliveTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#OliveTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#ParmesanTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#ParmesanTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#CheeseTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperoniTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#PepperoniTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/heinz/ontologies/2025/12/pizza#Pizza">
